    "app/nativescript-angular/renderer.ts",
    "app/nativescript-angular/view_node.ts",
    "app/nativescript-angular/xhr.ts",
    "app/nativescript-angular/zone.ts",
    "app/nativescript-angular/zone_patch.ts",
    "app/profiling.ts",
    "app/renderer-test.ts",
