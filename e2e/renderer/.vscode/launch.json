{
        "version": "0.2.0",
        "configurations": [
            {
                "type": "node",
                "request": "launch",
                "name": "Mocha Tests",
                "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
                "args": [
                    "-u",
                    "tdd",
                    "--timeout",
                    "999999",
                    "--colors",
                    "${workspaceRoot}/e2e",
                    "-a",
                    "--grep",
                    "actionBarVisibility 'auto' shows action bars based on page"

                ],
                "internalConsoleOptions": "openOnSessionStart"
            }
        ]
    }